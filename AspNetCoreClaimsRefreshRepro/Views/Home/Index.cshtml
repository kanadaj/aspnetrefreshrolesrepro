@{
    ViewData["Title"] = "Home Page";
}

<div>
    <h2>Current Claims:</h2>
    <ul>
        @foreach (var claim in User.Claims)
        {
            <li>
                <strong>@claim.Type</strong> @claim.Value
            </li>
        }
    </ul>
</div>

<a class="btn" asp-action="AddToRole">Add to role</a>